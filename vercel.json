{
    "version": 2,
    "builds": [
        {
            "source": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "source": "/(.*)",
            "destination": "./src/api/routes/users.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "./public/index.html"
        }
    ]
}